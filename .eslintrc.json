{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-use-before-define": ["error", { "functions": false, "classes": true }],
      "indent": ["error", 2],
      "prefer-destructuring": ["error", {
        "array": false,
        "object": false
      }, {
        "enforceForRenamedProperties": false
      }],
      "no-console": "off"

    }
}
